@model IEnumerable<Student.Models.Parent>

@{
    ViewBag.Title = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>View</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Pa_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pa_sex)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Pa_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pa_sex)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ParentID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ParentID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ParentID })
        </td>
    </tr>
}

</table>
